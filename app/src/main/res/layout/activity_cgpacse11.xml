<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cgpacse11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/blackbg"
    tools:context="com.example.hp.austguide.cgpacse11">
    <TextView
        android:id="@+id/hum"
        android:layout_marginTop="10dp"
        android:layout_width="160dp"
        android:text="HUM1107  : "
        android:textStyle="bold"
android:paddingBottom="0dp"
        android:textSize="26dp"
        android:paddingTop="10dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_height="40dp" />
    <EditText
        android:inputType="number"

        android:id="@+id/hummarks"
        android:layout_marginTop="10dp"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_toRightOf="@+id/hum"/>
    <TextView
        android:id="@+id/chem"
        android:text="CHEM1115: "
        android:textStyle="bold"

        android:textSize="26dp"
        android:paddingTop="10dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_below="@+id/hum"
        android:layout_width="160dp"
        android:layout_height="40dp" />
    <EditText
        android:id="@+id/chemmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:inputType="number"

        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_toRightOf="@+id/chem"
        android:layout_below="@+id/hummarks"/>
    <TextView
        android:layout_marginTop="0dp"
        android:layout_below="@+id/chem"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:text="PHY1115   : "
        android:textStyle="bold"
android:id="@+id/phy"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"/>
    <EditText
        android:inputType="number"

        android:id="@+id/phymarks"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:layout_toRightOf="@+id/phy"
        android:layout_below="@+id/chemmarks"/>
    <TextView
        android:text="CSE1101    : "
        android:textStyle="bold"
        android:id="@+id/cse"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/phy"
        />
    <EditText
        android:inputType="number"

        android:id="@+id/csemarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/phymarks"
        android:layout_toRightOf="@+id/cse"/>
    <!--
    Mathematics-I MATH1115
-->
    <TextView
        android:text="MATH1115: "
        android:textStyle="bold"
        android:id="@+id/math"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/cse"
        />

    <EditText
        android:inputType="number"

        android:id="@+id/mathmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/csemarks"
        android:layout_toRightOf="@+id/math"/>
    <TextView
        android:text="HUM LAB   : "
        android:textStyle="bold"
        android:id="@+id/humlab"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/math"
        />

    <EditText
        android:inputType="number"

        android:id="@+id/humlabmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/mathmarks"
        android:layout_toRightOf="@+id/math"/>

    <TextView
        android:text="PHY LAB    : "
        android:textStyle="bold"
        android:id="@+id/phylab"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/humlab"
        />
    <EditText
        android:inputType="number"

        android:id="@+id/phylabmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/humlabmarks"
        android:layout_toRightOf="@+id/phylab"/>
    <TextView
        android:text="C LAB         : "
        android:textStyle="bold"
        android:id="@+id/clab"
        android:textSize="26dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/phylab"
        />
    <EditText
        android:inputType="number"

        android:id="@+id/clabmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/phylabmarks"
        android:layout_toRightOf="@+id/clab"/>
<!--   CSE1108  -->
    <TextView
        android:text="CSE1108LAB : "
        android:textStyle="bold"
        android:id="@+id/c1108lab"
        android:textSize="22dp"
        android:paddingTop="5dp"
        android:paddingLeft="9dp"
        android:textColor="#f9e806"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:layout_below="@+id/clab"
        />
    <EditText
        android:inputType="number"

        android:id="@+id/c1108labmarks"
        android:layout_width="191dp"
        android:layout_height="40dp"
        android:background="@drawable/border_style"
        android:textColor="#FFFFFDFD"
        android:hint="enter marks"
        android:textColorHint="#FFFFFDFD"
        android:textSize="23dp"
        android:paddingLeft="9dp"
        android:layout_below="@+id/clabmarks"
        android:layout_toRightOf="@+id/c1108lab"/>

    <Button
        android:onClick="onclicks"
        android:background="@drawable/border_style"
        android:textSize="20dp"
        android:textColor="#FFFFFDFD"
        android:text="CALCULATE"
        android:layout_marginLeft="69dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/c1108lab"
        android:id="@+id/resultd"
        android:layout_width="170dp"
        android:layout_height="70dp" />
    <TextView
        android:layout_marginTop="14dp"
        android:id="@+id/resultview"
        android:textColor="#dc23d9"

        android:layout_below="@+id/resultd"
        android:layout_width="200dp"
        android:layout_marginLeft="60dp"
        android:layout_height="40dp" />
</RelativeLayout>
