<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cgpacse"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/semesterpagenew"
    tools:context="com.example.hp.austguide.cgpacse">
    <TextView
        android:id="@+id/id01"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:text="CHOOSE YOUR SEMESTER"
        android:textStyle="bold"
        android:textSize="23dp"
        android:textColor="#e70e7b"

        android:layout_height="50dp" />
    <Button
        android:id="@+id/id02"
        android:onClick="onClick56"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="1st year 1st semester"
        android:textColor="#FFFFFDFD"

        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id01"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="onClick57"
        android:id="@+id/id03"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="1st year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id02"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="onClick58"
        android:id="@+id/id04"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="2nd year 1st semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id03"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="onClick59"
        android:id="@+id/id05"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="2nd year  2nd  semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id04"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />

    <Button
        android:onClick="onClick60"
        android:id="@+id/id06"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="3rd year 1st semester"
        android:textColor="#ffffff"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id05"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />



    <Button
        android:onClick="onClick61"
        android:id="@+id/id07"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="3rd year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id06"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="onClick62"
        android:id="@+id/id08"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="4th year 1st semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id07"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="onClick63"
        android:id="@+id/id09"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="4th year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id08"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />


</RelativeLayout>
