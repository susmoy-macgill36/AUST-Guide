<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_allresultthis"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/secondpage02"
    tools:context="com.example.hp.austguide.Allresultthis">

    <TextView
        android:id="@+id/id1w"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:text="CHOOSE YOUR DEPARTMENT"
        android:textStyle="bold"
        android:textSize="23dp"
        android:textColor="#ea0d7c"
        android:layout_height="50dp" />
    <Button
        android:id="@+id/id2w"
android:onClick="arcsemester"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of Architecture"
        android:textColor="#FFFFFDFD"

        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id1w"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        />
    <Button
android:onClick="eeesemester"
        android:id="@+id/id3w"

        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of EEE"
        android:textColor="#FFFFFDFD"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id2w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:id="@+id/id4w"
        android:onClick="semsester"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of cse"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id3w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />
    <Button

        android:id="@+id/id5w"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of TE"
        android:textColor="#FFFFFDFD"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id4w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />
    <Button

        android:id="@+id/id6w"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of ce"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id5w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />
    <Button

        android:id="@+id/id7w"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text=" Department of mpe"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id6w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />
    <Button

        android:id="@+id/id8w"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="School of Business (SoB)"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"

        android:layout_below="@+id/id7w"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="0dp"
        />




</RelativeLayout>
