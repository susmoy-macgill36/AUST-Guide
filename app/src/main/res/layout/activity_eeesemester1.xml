        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as5"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />



    <Button
        android:onClick="eeeres32"
        android:id="@+id/id0as7"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="3rd year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as6"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="eeeres41"
        android:id="@+id/id0as8"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="4th year 1st semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as7"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="eeeres42"
        android:id="@+id/id0as9"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="4th year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as8"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />

