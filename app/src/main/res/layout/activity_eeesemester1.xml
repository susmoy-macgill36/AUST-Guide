<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_resultsemester"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/semesterpagenew"

    tools:context="com.example.hp.austguide.Eeesemester1">
    <TextView
        android:id="@+id/id0as1"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:text="CHOOSE YOUR SEMESTER"
        android:textStyle="bold"
        android:textSize="23dp"
        android:textColor="#e70e7b"

        android:layout_height="50dp" />
    <Button
        android:id="@+id/id0as2"
android:onClick="eeeres11"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="1st year 1st semester"
        android:textColor="#FFFFFDFD"

        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="eeeres12"
        android:id="@+id/id0as3"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="1st year 2nd semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as2"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="eeeres21"
        android:id="@+id/id0as4"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:text="2nd year 1st semester"
        android:textColor="#fffdfd"
        android:background="@drawable/border_style"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/id0as3"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="0dp"
        />
    <Button
        android:onClick="eeeres22"
        android:id="@+id/id0as5"
        android:layout_width="match_parent"
        android:layout_height="45dp"
